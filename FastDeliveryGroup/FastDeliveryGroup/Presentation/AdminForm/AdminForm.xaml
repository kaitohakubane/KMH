            <ListBoxItem Content="Product Management"/>
            <ListBoxItem Content="District Management"/>
            <ListBoxItem Content="Shipper Management"/>
            <ListBoxItem Content="Report"/>
        </ListBox>
        <StackPanel x:Name="pnlUserControl" HorizontalAlignment="Left" Height="627" Margin="184,19,0,0" VerticalAlignment="Top" Width="741"/>

    </Grid>
</Window>
