        xmlns:StaffForm="clr-namespace:FastDeliveryGroup.Presentation.StaffForm" x:Name="frmAdmin" x:Class="FastDeliveryGroup.Presentation.AdminForm.AdminForm"
        mc:Ignorable="d"
        Title="AdminForm" Height="700" Width="1000">
