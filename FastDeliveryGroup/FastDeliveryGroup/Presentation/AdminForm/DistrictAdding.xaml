<Window x:Class="FastDeliveryGroup.Presentation.StaffForm.AddDistrict"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastDeliveryGroup.Presentation.StaffForm"
        mc:Ignorable="d"
        Title="AddDistrict" Height="300" Width="600">
    <Grid>
        <Label x:Name="label1" Content="Name Of District" HorizontalAlignment="Left" Margin="30,50,0,0" VerticalAlignment="Top" FontFamily="Times New Roman" FontSize="16"/>
        <TextBox x:Name="txtNameOfDistrict" HorizontalAlignment="Left" Height="27" Margin="194,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="250" FontFamily="Times New Roman" FontSize="16"/>
        <Button x:Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="217,157,0,0" VerticalAlignment="Top" Width="75" FontFamily="Times New Roman" FontSize="20" FontWeight="Bold" Click="btnOk_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="332,157,0,0" VerticalAlignment="Top" Width="98" FontFamily="Times New Roman" FontSize="20" FontWeight="Bold" Click="btnCancel_Click"/>
    </Grid>
</Window>
